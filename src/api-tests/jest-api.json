{
    "moduleFileExtensions": [
        "js",
        "json",
        "ts"
    ],
    "rootDir": ".",
    "testEnvironment": "node",
    "testRegex": ".api.test.ts$",
    "testPathIgnorePatterns": [
        "node_modules"
    ],
    "transform": {
        "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverage": false
}